  "repository": {
    "type": "git",
    "url": "https://github.com/aliaksandrsurma/cv-as-a-project-ui.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.3",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "remark-gfm": "^4.0.0",
    "styled-components": "^6.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "env-cmd": "^10.1.0",
    "gh-pages": "^6.1.1",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "semver": "^7.5.4",
    "source-map-explorer": "^2.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rimraf ./build && react-scripts build",
    "build:local": "env-cmd -f .env.local npm run build",
    "build:prod": "env-cmd -f .env.production npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy:prod": "npm run build:prod",
    "deploy:prod": "gh-pages -d build",
    "deploy-patch": "npm version patch -m 'Releasing %s version' && npm run deploy:prod",
    "deploy-minor": "npm version minor && npm run deploy:prod",
    "deploy-major": "npm version major && npm run deploy:prod",
    "postdeploy-patch": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
